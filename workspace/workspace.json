{
  "version": 2,
  "cli": { "defaultCollection": "@nrwl/react" },
  "defaultProject": "idonotknow",
  "generators": {
    "@nrwl/react": {
      "application": { "style": "scss", "linter": "eslint", "babel": true },
      "component": { "style": "scss" },
      "library": { "style": "scss", "linter": "eslint" }
    }
  },
  "projects": {
    "idonotknow": {
      "root": "apps/idonotknow",
      "sourceRoot": "apps/idonotknow/src",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/web:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/idonotknow",
            "index": "apps/idonotknow/src/index.html",
            "main": "apps/idonotknow/src/main.tsx",
            "polyfills": "apps/idonotknow/src/polyfills.ts",
            "tsConfig": "apps/idonotknow/tsconfig.app.json",
            "assets": [
              "apps/idonotknow/src/favicon.ico",
              "apps/idonotknow/src/assets"
            ],
            "styles": ["apps/idonotknow/src/styles.scss"],
            "scripts": [],
            "webpackConfig": "@nrwl/react/plugins/webpack"
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/idonotknow/src/environments/environment.ts",
                  "with": "apps/idonotknow/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@nrwl/web:dev-server",
          "options": { "buildTarget": "idonotknow:build" },
          "configurations": {
            "production": { "buildTarget": "idonotknow:build:production" }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/idonotknow/**/*.{ts,tsx,js,jsx}"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/idonotknow"],
          "options": {
            "jestConfig": "apps/idonotknow/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "idonotknow-e2e": {
      "root": "apps/idonotknow-e2e",
      "sourceRoot": "apps/idonotknow-e2e/src",
      "projectType": "application",
      "targets": {
        "e2e": {
          "executor": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/idonotknow-e2e/cypress.json",
            "tsConfig": "apps/idonotknow-e2e/tsconfig.e2e.json",
            "devServerTarget": "idonotknow:serve"
          },
          "configurations": {
            "production": { "devServerTarget": "idonotknow:serve:production" }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/idonotknow-e2e/**/*.{js,ts}"]
          }
        }
      }
    },
    "my-lib": {
      "root": "libs/my-lib",
      "sourceRoot": "libs/my-lib/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/my-lib/**/*.{ts,tsx,js,jsx}"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/my-lib"],
          "options": {
            "jestConfig": "libs/my-lib/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    }
  }
}
